@model WebSite.Models.CARGOS

@{
    ViewBag.Title = "Cargos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("~/Views/CARGOS/_Partial_NavPills_Cargos.cshtml")

<div class="col-md-9">
    <h3>¿Estas Seguro que desea eliminar el cargo?</h3>
    <div>

        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayName("ID")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ID_CARGOS)
            </dd>
            <dt>
                @Html.DisplayName("Cargo")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.NOMBRE)
            </dd>

            <dt>
                @Html.DisplayName("Area")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AREAS.NOMBRE)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Eliminar" class="btn btn-success" /> |
                <input type="button" value="Regresar" class="btn btn-danger" onclick="location.href='@Url.Action("Index", "CARGOS" )'" />
            </div>
        }
    </div>
</div>