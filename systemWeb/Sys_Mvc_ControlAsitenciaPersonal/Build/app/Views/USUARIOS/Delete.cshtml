@model WebSite.Models.USUARIOS

@{
    ViewBag.Title = "Usuarios";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("~/Views/USUARIOS/_Partial_NavPills_Usuario.cshtml")

<div class="col-md-9">
    <h2>Eliminar Usuarios</h2>

    <h3>¿Estas Seguro que desea eliminar el usuarios?</h3>
    <div>

        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayName("ID Usuario")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ID_USUARIO)
            </dd>

            <dt>
                @Html.DisplayName("Tipo");
            </dt>

            <dd>
                @Html.DisplayFor(model => model.TIPO)
            </dd>

            <dt>
                @Html.DisplayName("Activo");
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ACTIVO)
            </dd>

            <dt>
                @Html.DisplayName("Empleado")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.EMPLEADOS.NOMBRES)&nbsp;@Html.DisplayFor(model => model.EMPLEADOS.APELLIDOS)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Eliminar" class="btn btn-success" /> |
                <input type="button" value="Regresar" class="btn btn-danger" onclick="location.href='@Url.Action("Index", "USUARIOS" )'" />
            </div>
        }
    </div>
    </div>
